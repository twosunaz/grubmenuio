{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "orderable/order-line-items",
	"version": "1.0.0",
	"title": "Orderable - Order Line Items",
	"category": "orderable",
	"description": "Shows the order line items.",
	"attributes": {
		"showLabel": {
			"type": "boolean",
			"default": true
		},
		"label": {
			"type": "string",
			"default": "Order Items:"
		},
		"showPrices": {
			"type": "boolean",
			"default": true
		},
		"showCheckboxes": {
			"type": "boolean",
			"default": false
		},
		"showMetaData": {
			"type": "boolean",
			"default": false
		}
	},
	"textdomain": "orderable",
	"editorScript": "file:./index.js",
	"render": "file:./render.php",
	"editorStyle": "file:./index.css"
}
