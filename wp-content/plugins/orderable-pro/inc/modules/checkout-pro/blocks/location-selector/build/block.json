{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 2,
  "name": "orderable-pro/location-selector-block",
  "version": "1.0.0",
  "title": "Orderable - Location selector",
  "category": "woocommerce",
  "description": "Shows the Location selector if more than one location is available",
  "supports": {
    "html": false,
    "align": false,
    "multiple": false,
    "reusable": false
  },
  "parent": [
    "woocommerce/checkout-order-summary-block"
  ],
  "attributes": {
    "lock": {
      "type": "object",
      "default": {
        "remove": true,
        "move": false
      }
    }
  },
  "textdomain": "orderable-pro",
  "editorScript": "file:./index.js",
  "viewScript": "file:./view.js",
  "render": "file:./render.php",
  "style": "file:./style-index.css"
}